{
  "name": "mdm-docs-superinfo",
  "version": "1.0.0",
  "engines": {
    "node": "22.16.0",
    "npm": "11.8.0"
  },
  "type": "module",
  "scripts": {
    "gen-data": "node scripts/build-data.mjs",
    "prebuild": "npm run gen-data",
    "dev": "npm run gen-data && next dev -p 4000",
    "build": "next build",
    "build:cf": "npx @opennextjs/cloudflare build --bundle-external jose && cp .open-next/worker.js .open-next/_worker.js",
    "build:gh": "npm run gen-data && next build",
    "start": "next start -p 4000",
    "lint": "next lint",
    "prepare": "husky",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@logto/next": "^4.2.8",
    "@opennextjs/aws": "^3.9.12",
    "@opennextjs/cloudflare": "^1.15.1",
    "jose": "5.10.0",
    "lucide-react": "^0.563.0",
    "next": "16.1.4",
    "nextra": "^4.6.1",
    "nextra-theme-docs": "^4.6.1",
    "patch-package": "^8.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "swr": "^2.3.8"
  },
  "devDependencies": {
    "@aws-crypto/crc32": "^5.2.0",
    "@aws-crypto/crc32c": "^5.2.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "globby": "^16.1.0",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "markdown-it": "^14.1.0",
    "open-next": "^3.1.3",
    "postcss": "^8.5.1",
    "prettier": "^3.5.1",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  }
}