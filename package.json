    "build:static": "npm run gen-data && export GITHUB_ACTIONS=true && rm -rf app/api && next build",
    "start": "next start -p 4000",
    "lint": "next lint",
    "prepare": "husky"
  },
  "dependencies": {
    "@logto/next": "^4.2.8",
    "jose": "5.10.0",
    "lucide-react": "^0.563.0",
    "next": "16.1.4",
    "nextra": "^4.6.1",
    "nextra-theme-docs": "^4.6.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "swr": "^2.3.8"
  },
  "devDependencies": {
    "@aws-crypto/crc32": "^5.2.0",
    "@aws-crypto/crc32c": "^5.2.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.0",
    "globby": "^16.1.0",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "markdown-it": "^14.1.0",
    "postcss": "^8.5.1",
    "prettier": "^3.5.1",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.19.2",
