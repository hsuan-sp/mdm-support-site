{
  "name": "mdm-docs-superinfo",
  "version": "1.0.0",
  "engines": {
    "node": ">=24.13.0",
    "npm": ">=11.6.2"
  },
  "type": "module",
  "scripts": {
    "gen-data": "node scripts/build-data.mjs",
    "prebuild": "npm run gen-data",
    "dev": "npm run gen-data && next dev -p 4000",
    "build": "npx patch-package && next build && npx @opennextjs/cloudflare build --skipBuild",
    "build:gh": "npm run gen-data && next build",
    "start": "next start -p 4000",
    "lint": "next lint",
    "dev:clean": "node ./scripts/dev-clean.js",
    "update-index": "node scripts/maintenance/generate-index.js",
    "update-bundles": "node scripts/maintenance/generate-bundles.js",
    "fix-markdown": "node scripts/maintenance/unified-formatter.js",
    "format": "prettier --write .",
    "prepare": "husky",
    "postinstall": "patch-package" 
  },
  "dependencies": {
    "@logto/next": "^4.2.8",
    "jose": "5.10.0",
    "lucide-react": "^0.563.0",
    "next": "^15.5.9",
    "nextra": "^3.3.1",
    "nextra-theme-docs": "^3.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "swr": "^2.3.8"
  },
  "devDependencies": {
    "@aws-crypto/crc32": "^5.2.0",
    "@aws-crypto/crc32c": "^5.2.0",
    "@opennextjs/cloudflare": "^1.15.1",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.10.9",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.23",
    "globby": "^16.1.0",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "markdown-it": "^14.1.0",
    "open-next": "^3.1.3",
    "patch-package": "^8.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.19",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  }
}
